<number>#n -> # ^ <what>string2number(#n), # ! <number>.

(JumpSlide | JumpChapter)
{
  // default if not specified: 1
  ^ <theme>(Forward | Back) ^ !<what> ^ !<number>
  ->
  # ^ <what>1.

  ^ <theme>Forward ^ <what>
  ->
  # ! <theme>.

  ^ <theme>Back ^ <what>#w:
  ->
  # ! <theme>,
  #w = concatenate("-", #w).
}

:Out_Of_Domain ^ :#t -> #t = PossibleQuestion.

<mec> -> # ! <mec>.